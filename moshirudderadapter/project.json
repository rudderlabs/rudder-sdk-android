{
  "name": "moshirudderadapter",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "./moshirudderadapter/src",
  "targets": {
    "dependsOn": [
      {
        "projects":[
          "rudderjsonadapter",
          "models"
        ],
        "target":"build"
      }
    ],
    "build": {
      "executor": "@jnxplus/nx-gradle:build",
      "outputs": ["./moshirudderadapter/build"]
    },
    "lint": {
      "executor": "@jnxplus/nx-gradle:lint",
      "options": {
        "linter": "ktlint"
      }
    },
    "test": {
      "executor": "@jnxplus/nx-gradle:test"
    },
    "ktformat": {
      "executor": "@jnxplus/nx-gradle:ktformat"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "master",
        "preset": "conventional",
        "tagPrefix": "${projectName}@"
      }
    },
    "sync-bumped-version-properties": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node gradle-updater.js 'moshirudderadapter'"
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "moshirudderadapter@0.1.0",
        "notesFile": "./moshirudderadapter/CHANGELOG_LATEST.md"
      }
    },
    "release-sonatype": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'moshirudderadapter-release' && cd moshirudderadapter && sh ../gradlew generatePomFileForReleasePublication && ../gradlew publishToSonatype  && cd ..  && ./gradlew findSonatypeStagingRepository closeSonatypeStagingRepository && ./gradlew findSonatypeStagingRepository releaseSonatypeStagingRepository"
      }
    },
    "snapshot-release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'moshirudderadapter-snapshot' && cd moshirudderadapter && sh ../gradlew generatePomFileForReleasePublication && ../gradlew publishToSonatype && cd .."
      }
    }
  },
  "tags": []
}
