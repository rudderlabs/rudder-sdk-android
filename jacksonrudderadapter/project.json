{
  "name": "jacksonrudderadapter",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "./jacksonrudderadapter/src",
  "targets": {
    "build": {
      "dependsOn": [
        {
          "projects":[
            "rudderjsonadapter",
            "models"
          ],
          "target":"build"
        }
      ],
      "executor": "@jnxplus/nx-gradle:build",
      "outputs": ["./jacksonrudderadapter/build"]
    },
    "lint": {
      "dependsOn": ["build"],
      "executor": "@jnxplus/nx-gradle:lint",
      "options": {
        "linter": "ktlint"
      }
    },
    "test": {
      "dependsOn": ["build"],
      "executor": "@jnxplus/nx-gradle:test"
    },
    "ktformat": {
      "executor": "@jnxplus/nx-gradle:ktformat"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "master",
        "preset": "conventional",
        "tagPrefix": "${projectName}@"
      }
    },
    "sync-bumped-version-properties": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node gradle-updater.js 'jacksonrudderadapter'"
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "jacksonrudderadapter@0.1.0",
        "notesFile": "./jacksonrudderadapter/CHANGELOG_LATEST.md"
      }
    },
    "release-sonatype": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'jacksonrudderadapter-release' && cd jacksonrudderadapter && sh ../gradlew generatePomFileForReleasePublication && ../gradlew publishToSonatype  && cd ..  && ./gradlew findSonatypeStagingRepository closeSonatypeStagingRepository && ./gradlew findSonatypeStagingRepository releaseSonatypeStagingRepository"
      }
    },
    "snapshot-release": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'jacksonrudderadapter-snapshot' && cd jacksonrudderadapter && sh ../gradlew generatePomFileForReleasePublication && ../gradlew publishToSonatype && cd .."
      }
    }
  },
  "tags": []
}
